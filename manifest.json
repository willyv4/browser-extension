{
  "manifest_version": 3,
  "name": "WILL Xtension",
  "version": "1.0",
  "description": "This is a basic Chrome extension.",
  "action": {
    "default_popup": "index.html",
    "default_icon": "mypic.jpg"
  },
  "permissions": ["tabs"],
  "host_permissions": ["https://*/*", "http://*/*"],
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["content.js"]
    }
  ]
}
